{
  "name": "vlack",
  "version": "0.4.0",
  "description": "vlack is a black-based UI toolkit for React.",
  "keywords": [
    "UI",
    "UI Framework",
    "Component Library"
  ],
  "homepage": "https://github.com/xrxoxcxox/vlack#readme",
  "license": "MIT",
  "author": "Keisuke Watanuki",
  "files": [
    "dist/"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "repository": "github:xrxoxcxox/vlack",
  "scripts": {
    "build": "rimraf dist && tsc --emitDeclarationOnly && vite build",
    "chromatic": "chromatic",
    "format": "npm-run-all -p format:*",
    "format:eslint": "eslint . --fix",
    "format:prettier": "prettier . --write",
    "format:stylelint": "stylelint . --fix",
    "lint": "npm-run-all -p lint:*",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier . --check",
    "lint:stylelint": "stylelint .",
    "prepack": "npm run build",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/react": "^11.9.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@storybook/addon-actions": "^7.0.2",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-links": "^7.0.2",
    "@storybook/addon-mdx-gfm": "^7.0.2",
    "@storybook/react": "^7.0.2",
    "@storybook/react-vite": "^7.0.2",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "babel-loader": "^8.2.5",
    "chromatic": "^6.5.4",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-storybook": "^0.6.11",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "storybook": "^7.0.2",
    "stylelint": "^14.8.5",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.7.2",
    "vite": "^4.2.1"
  },
  "peerDependencies": {
    "@emotion/react": "^11.9.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "engines": {
    "node": ">=18"
  },
  "volta": {
    "node": "18.15.0"
  }
}
